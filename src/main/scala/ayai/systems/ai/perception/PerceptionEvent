package ayai.systems

import ayai.components._
import ayai.actions._

import crane.{Component, Entity, World, EntityProcessingSystem}

import akka.actor.ActorSystem

import scala.collection.mutable.ArrayBuffer

object PerceptionEvent {
}

class PerceptionEvent(main: Character, target: Character, action: String) {
  def getMain(): Charactor = {
    return main;
  }

  def getTarget(): Charactor = {
    return target;
  }

  def getAction(): String = {
    return action;
  }
}